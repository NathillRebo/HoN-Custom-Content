							midspawnratepos=".99"
							minparticlelife="50"
							maxparticlelife="200"
							mincount="5"
							maxcount="15"
							material="/shared/effects/materials/whitedot.material"
							minspeed="75"
							maxspeed="150"
							drag="0.05"
							gravity="50"
						>
							<particle
								startcolor=".2 .5 .5"
								midcolor=".3 .5 .5"
								endcolor="0"
								scale=".5"
								minwidth=".5"
								maxwidth="1"
